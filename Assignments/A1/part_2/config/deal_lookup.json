        "method": "GET",
		"headers": null,
		"params": {
			"id": true
		},
		"body": null,
		"cookies": null,
		"authorization": null,
		"pagination": null,
		"search": null
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$",
        "schema": {
			"storeID" : {
				"target": "$.gameInfo.storeID",
