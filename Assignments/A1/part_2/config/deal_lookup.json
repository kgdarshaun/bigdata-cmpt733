				"type": "int"
			},
			"CheapestStoreSalesPrice": {
				"target": "$.cheaperStores.salePrice",
				"type": "float"
			},
			"cheapestPrice": {
				"target": "$.cheapestPrice.price",
				"type": "float"
			},
			"cheapestPriceDate": {
				"target": "$.cheapestPrice.date",
				"type": "int"
