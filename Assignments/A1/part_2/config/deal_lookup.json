			},
			"gameID" : {
				"target": "$.gameInfo.gameID",
				"type": "int"
			},
			"name" : {
				"target": "$.gameInfo.name",
				"type": "string"
			},
			"steamAppID": {
				"target": "$.gameInfo.steamAppID",
				"type": "int"
			},
			"salePrice": {
				"target": "$.gameInfo.salePrice",
				"type": "float"
			},
			"retailPrice": {
				"target": "$.gameInfo.retailPrice",
				"type": "float"
			},
			"steamRatingText": {
				"target": "$.gameInfo.steamRatingText",
				"type": "string"
			},
			"steamRatingPercent": {
				"target": "$.gameInfo.steamRatingPercent",
				"type": "int"
			},
			"steamRatingCount": {
				"target": "$.gameInfo.steamRatingCount",
